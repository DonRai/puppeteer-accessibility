{
  "name": "accessibility-perf",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "next"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:accessibility": "JEST_PUPPETEER_CONFIG=./accessibility/jest-puppeteer.config.js jest --runInBand --verbose --colors --no-cache --config ./accessibility/jest.config.json",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "axe-core": "^3.0.3",
    "babel-preset-env": "^1.7.0",
    "jest-puppeteer": "^3.2.1",
    "ms": "^2.1.1",
    "puppeteer": "^1.5.0",
    "serve": "^9.2.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
